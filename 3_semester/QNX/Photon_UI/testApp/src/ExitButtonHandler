/* Callback Template                                     */

/* Standard headers */
#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <string.h>

/* Toolkit headers */
#include <Ph.h>
#include <Pt.h>
#include <Ap.h>

int
callback( PtWidget_t *widget, void *data, PtCallbackInfo_t *cbinfo )

	{

	/* eliminate 'unreferenced' warnings */
	widget = widget; data = data; cbinfo = cbinfo;

	return( Pt_CONTINUE );

	}

